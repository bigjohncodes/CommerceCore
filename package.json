        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/express-ejs-layouts": "^2.5.4",
        "@types/faker": "^6.6.8",
        "@types/formidable": "^3.4.5",
        "@types/helmet": "^0.0.48",
        "@types/jest": "^29.5.14",

{
    "name": "shopee-clone-be",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "_moduleAliases": {
        "~": "./src"
    },
    "scripts": {
        "dev": "npx nodemon",
        "start": "npm run build:live",
        "build": "rimraf ./dist && tsc && tsc-alias && cpx src/**/*.{yaml,html,sql,json} dist && cpx openapi/openapi.yaml dist/openapi",
        "build:live": "\"node\" --max-old-space-size=4096 dist/index.js",
        "build:pm": "npx pm2 start dist/index.js",
        "stop:pm": "npx pm2 stop dist/index.js",
        "rebuild:pm": "npm run stop:pm && npm run build && npm run build:pm",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "test": "jest",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:create": "npx typeorm-ts-node-commonjs migration:create",
        "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/dbs/db.ts ",
        "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/dbs/db.ts",
        "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/dbs/db.ts",
        "apidocs": "swagger-cli bundle ./openapi/_index.yaml --outfile ./openapi/openapi.yaml --type yaml",
        "seed:config": "npx ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "npx ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
    },
    "author": "",
    "license": "ISC",
